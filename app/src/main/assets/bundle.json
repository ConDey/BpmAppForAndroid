{
  "version": "1.0.0",
  "bundles": [
    {
      "uri": "lib.style",
      "pkg": "com.eazytec.bpm.lib.style"
    },
    {
      "uri": "lib.utils",
      "pkg": "com.eazytec.bpm.lib.utils"
    },
    {
      "uri": "lib.common",
      "pkg": "com.eazytec.bpm.lib.common"
    },
    {
      "uri": "app.webkit",
      "pkg": "com.eazytec.bpm.app.webkit"
    },
    {
      "uri": "app.home",
      "pkg": "com.eazytec.bpm.app.home"
    },
    {
      "uri": "app.contact",
      "pkg": "com.eazytec.bpm.app.contact",
      "rules": {
        "forhomefragment": "HomeContactFragment",
        "forcontactchoose": ".usercontact.contactchoose.UserChoose"
      }
    },
    {
      "uri": "app.notice",
      "pkg": "com.eazytec.bpm.app.notice",
      "rules": {
        "fornoticedetail": ".notice.detail.NoticeDetailActivity"
      }
    },
    {
      "uri": "app.photo",
      "pkg": "com.eazytec.bpm.app.photo"
    },
    {
      "uri": "app.filepicker",
      "pkg": "com.eazytec.bpm.app.filepicker",
      "rules": {
        "forfilepicker": ".filepicker.FilePicker"
      }
    },
    {
      "uri": "app.message",
      "pkg": "com.eazytec.bpm.app.message",
      "rules": {
        "forhomefragment": "HomeMessageFragemnt"
      }
    },
    {
      "uri": "app.calendar",
      "pkg": "com.eazytec.bpm.app.calendar"
    }
  ]
}
